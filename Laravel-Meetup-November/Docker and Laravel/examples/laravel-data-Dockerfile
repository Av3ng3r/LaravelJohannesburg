# Laravel Data
#
# docker build -t="laravelmeetup/laravel-data" .
#
# Launch
# docker run --name laravel-data laravelmeetup/laravel-data
#
# version 1.0

FROM laravelmeetup/centos7-base
MAINTAINER Warren Baker "warren@thepackethub.co.za" version: 1.0

RUN mkdir -p /var/www/data/
VOLUME ["/var/www/data/"]
CMD ["true"]